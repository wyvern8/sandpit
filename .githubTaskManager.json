{
  "pull_request": {
    "agentGroup": "Neko",
    "tasks": [
      {
        "executor": "Http",
        "context": "avtest",
        "options": {
          "har": {
            "url": "https://httpbin.org/headers",
            "method": "GET",
            "headers": [
              {
                "name": "accept",
                "value": "application/json"
              }  
            ]
          },
          "validator": {
            "type": "bodyJson",
            "objectCheckRef": "headers.Host",
            "objectCheckVal": "httpbin.org"
          }  
        }
      },
      {
        "executor": "Jenkins",
        "context": "functional",
        "options": {
          "tags": ["@nekoagent", "@smoketest", "@##GHREPONAME##"]
        }
      },
      {
        "executor": "Jenkins",
        "context": "a11y",
        "options": {
          "tags": ["@a11y", "@##GHREPONAME##"]
        }
      },
      {
        "executor": "Jenkins",
        "context": "Intentionally Missing",
        "options": {
          "tags": ["None"]
        }
      }
    ]
  }  
}
